{
    "extends": [
        "airbnb",
        "prettier/react"
    ],
    "plugins": [
        "prettier"
    ],
    "env": {
        "browser": true,
        "jasmine": true
    },
    "rules": {
        "react/jsx-filename-extension": [1, {"extensions": [".js"]}],
        "arrow-parens": ["error", "as-needed"],
        "function-paren-newline": ["error", "never"],
        "prettier/prettier": ["error", { "singleQuote": true, "trailingComma": "all" }]
    }
}